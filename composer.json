{
    "name": "nhzex/simple-rpc",
    "description": "simple-rpc",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-sockets": "*",
        "ext-swoole": "~4.3",
        "ext-zlib": "*",
        "psr/container": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "nhzex/tp-swoole": "2.0.x-dev",
        "phpunit/phpunit": "^8.1",
        "swoft/swoole-ide-helper": "^4.4",
        "topthink/framework": "6.0.x-dev#f0babbd6529a57d8043690d63aa9505ca8a2c60d",
        "topthink/think-orm": "2.0.x-dev#d0649a61896ddd060e7a038b79b3481d8730ceff"
    },
    "suggest": {
        "nhzex/tp-swoole": "must be installed v2.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\SimpleRpc\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\SimpleRpc\\Stub\\": "stub/",
            "HZEX\\SimpleRpc\\Tests\\": "tests/"
        },
        "files": [
            "stub/common.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
    }
}
