{
    "name": "nhzex/simple-rpc",
    "description": "simple-rpc",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-sockets": "*",
        "ext-swoole": "~4.3",
        "ext-zlib": "*"
    },
    "require-dev": {
        "nhzex/tp-swoole": "2.0.x-dev",
        "phpunit/phpunit": "^8.1",
        "swoole/ide-helper": "^4.3"
    },
    "suggest": {
        "nhzex/tp-swoole": "must be installed v2.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\SimpleRpc\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\SimpleRpc\\Stub\\": "stub/",
            "HZEX\\SimpleRpc\\Tests\\": "tests/"
        },
        "files": [
            "stub/common.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
