{
    "name": "nhzex/simple-rpc",
    "description": "simple-rpc",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-sockets": "*",
        "ext-swoole": "~4.3",
        "ext-zlib": "*",
        "ext-posix": "*",
        "nhzex/zsw-core": "dev-master",
        "psr/container": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "swoft/swoole-ide-helper": "^4.4",
        "symfony/process": "^4.3",
        "symfony/var-dumper": "^4.3"
    },
    "suggest": {
        "nhzex/tp-swoole": "must be installed v2.0"
    },
    "autoload": {
        "psr-4": {
            "HZEX\\SimpleRpc\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\SimpleRpc\\Stub\\": "stub/",
            "HZEX\\SimpleRpc\\Tests\\": "tests/"
        },
        "files": [
            "stub/common.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
    }
}
